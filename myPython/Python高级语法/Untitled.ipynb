    "# 递归案例\n",
    "def digui(num):\n",
    "    print(\"@\"+str(num))\n",
    "    if num > 0:\n",
    "        digui(num-1)\n",
    "\n",
    "    else:\n",
    "        print(\"=\"*20)\n",
